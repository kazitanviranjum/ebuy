* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    outline: none;
}

a {
    text-decoration: none;
}

ul {
    list-style: none;
}

.container {
    width: 1180px;
    margin: 0 auto;
}

.row {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

body {
    font-family: 'Lato', sans-serif;
}


/*----Topbar CSS Start----*/

#main_top {
    background-color: #444444;
}

#main_top .row {
    height: 49px;
}

#main_top .left {
    color: #dddddd;
    position: relative;
}

#main_top .left::after {
    content: '';
    background-color: #dddddd;
    width: 1px;
    height: 47px;
    top: -14px;
    left: 61px;
    bottom: 0;
    right: 0;
    display: inline-block;
    position: absolute;
}

#main_top .left::before {
    content: '';
    background-color: #dddddd;
    width: 1px;
    height: 47px;
    top: -14px;
    left: 149px;
    bottom: 0;
    right: 0;
    display: inline-block;
    position: absolute;
}

#main_top .left .lan {
    background: transparent;
    color: #dddddd;
    border-color: transparent;
	cursor:pointer;
}
#main_top .left .lan:hover{
	color:#DF5F5D;
}

#main_top .left .money {
    background: transparent;
    color: #dddddd;
    border-color: transparent;
    padding-left: 20px;
	cursor:pointer;
}
#main_top .left .money:hover{
	color:#DF5F5D;
}

#main_top .left .tel {
    display: inline-block;
    padding-left: 20px;
	cursor:pointer;
}
#main_top .left .tel:hover{
	color:#DF5F5D;
}

#main_top .right {
    color: #dddddd;
}

#main_top .right .wish {
    display: inline-block;
}

#main_top .right .wish p {
    display: inline-block;
	cursor:pointer;
	
}
#main_top .right .wish:hover{
	color:#DF5F5D;
}

#main_top .right .my_ac {
    display: inline-block;
    padding-left: 30px;
    position: relative;
	
}

#main_top .right .my_ac::after {
    content: '';
    background-color: #dddddd;
    width: 1px;
    height: 47px;
    top: -15px;
    left: 146px;
    bottom: 0;
    right: 0;
    display: inline-block;
    position: absolute;
}

#main_top .right .my_ac::before {
    content: '';
    background-color: #dddddd;
    width: 1px;
    height: 47px;
    top: -15px;
    left: 14px;
    bottom: 0;
    right: 0;
    display: inline-block;
    position: absolute;
}

#main_top .right .my_ac p {
    display: inline-block;
	cursor:pointer;
}
#main_top .right .my_ac:hover{
	color:#DF5F5D;
}
#main_top .right .log {
    display: inline-block;
    padding-left: 30px;
	cursor:pointer;
}
#main_top .right .log:hover{
	color:#DF5F5D;
}



/*----Topbar CSS End----*/


/*----Navbar CSS Start----*/

#navbar {
    background-color: #e6e0e0;
    position: sticky;
    top: -1px;
    z-index: 3;
}

#navbar .row {
    height: 110px;
}

#navbar .logo img {
    width: 100%;
}

#navbar nav ul {
    display: flex;
}

#navbar nav ul li {
    position: relative;
    transition: all .4s linear;
}

#navbar nav ul li::after {
    position: absolute;
    content: '';
    background-color: #de4644;
    height: 0px;
    width: 0px;
    display: inline-block;
    bottom: -2px;
    left: 15px;
    transition: all .4s linear;
}

#navbar nav ul li:hover::after {
    background-color: #de4644;
    height: 1px;
    width: 39px;
}

#navbar nav ul li::before {
    position: absolute;
    content: '';
    background-color: #de4644;
    height: 0px;
    width: 0px;
    display: inline-block;
    bottom: -5px;
    left: 15px;
    transition: all .4s linear;
}

#navbar nav ul li:hover::before {
    background-color: #de4644;
    height: 1px;
    width: 30px;
}

#navbar nav ul li a {
    color: #222222;
    font-size: 16px;
    padding: 10px 15px;
    transition: all .4s linear;
    font-weight: bold;
}

#navbar nav ul li a:hover {
    color: #de4644;
    background-color: rgb(201, 196, 199);
}

#navbar .item .item_detail p {
    display: inline-block;
}

#navbar .item {
    position: relative;
}

#navbar .item .search {
    position: absolute;
    left: -70px;
	cursor:pointer;
}

#navbar .item .item_detail:hover {
    color: #de4644;
    cursor: pointer;
}


/*----Navbar CSS End----*/

#banner {
    background-image: url(../images/banner-bg.jpg);
