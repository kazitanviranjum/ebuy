}

#banner .container .b_group .b2 a {
    color: #fff;
    font-size: 16px;
    border: 1px solid #fff;
    padding: 10px 15px;
    display: inline-block;
    border-radius: 3px;
    transition: all .4s linear;
}

#banner .container .b_group .b2 a:hover {
